<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/web/kendo.common.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/web/kendo.rtl.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/web/kendo.default.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/shared/examples-offline.css")">
    
    <script src="@Url.Content("~/Scripts/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.web.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo.aspnetmvc.min.js")"></script>
     <script src="@Url.Content("~/Scripts/kendo.timezones.min.js")"></script>
    <script src="@Url.Content("~/Scripts/console.min.js")"></script>
    <script src="@Url.Content("~/Scripts/prettify.min.js")"></script>

@* Include the JavaScript for the current culture *@

@{
    var culture = System.Threading.Thread.CurrentThread.CurrentCulture.ToString();
}

    <script src="@Url.Content("~/Scripts/cultures/kendo.culture." + culture + ".min.js")"></script>

@* Set the current culture *@

    <script>
        kendo.culture("@culture");
    </script>

    @RenderSection("HeadContent", false)
</head>
<body>
    <div class="page">
        @Html.Partial("~/Views/Shared/_Navigation.cshtml")

        <div id="example" class="k-content">
            @RenderBody()
        </div>

        @Html.Partial("~/Views/Shared/_Source.cshtml")

    </div>
</body>
</html>
