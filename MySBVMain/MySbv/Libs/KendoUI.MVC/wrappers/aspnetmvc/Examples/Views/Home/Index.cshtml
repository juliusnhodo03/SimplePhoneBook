             <li class="box">
                <a href="@(Url.RouteUrl("RazorDefault", new { controller = "Suite", suite = "dataviz" }))" class="imgPlate"><img src="@Url.Content("~/Content/shared/demos-dataviz.png")" /></a>
                <h2><a href="@(Url.RouteUrl("RazorDefault", new { controller = "Suite", suite = "dataviz" }))">DataViz examples</a></h2>
            </li>
             <li class="box">
                <a href="@(Url.RouteUrl("RazorDefault", new { controller = "Suite", suite = "mobile" }))" class="imgPlate"><img src="@Url.Content("~/Content/shared/demos-mobile.png")" /></a>
                <h2><a href="@(Url.RouteUrl("RazorDefault", new { controller = "Suite", suite = "mobile" }))">Mobile examples</a></h2>
